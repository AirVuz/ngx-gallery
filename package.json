{
  "name": "ngx-gallery",
  "version": "3.0.0-beta.0",
  "description": "Angular gallery directive that hooks the lightbox with the images automatically.",
  "homepage": "http://github.com/murhafsousli/ngx-gallery",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-gallery.git"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-gallery/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-demo": "ng serve demo",
    "start-demo-aot": "ng serve demo --aot",
    "build-demo": "ng build demo --prod --base-href https://murhafsousli.github.io/ngx-gallery/",
    "sass-ts": "tsc .config/styles.ts --lib es7 && node .config/styles.js projects/core/src/lib dist/core",
    "sass": "node .config/styles.js projects/core/src/lib dist/core",
    "init-build": "npm run build-core && npm run version && npm run link-core && npm run build-lightbox && npm run version && npm run link-lightbox && npm run build-gallerize && npm run version && npm run link-gallerize && npm run sass && npm run version",
    "build-all": "npm run build-core && npm run build-lightbox && npm run build-gallerize && npm run sass && npm run version",
    "build-core": "ng build core --prod",
    "build-lightbox": "ng build lightbox --prod",
    "build-gallerize": "ng build gallerize --prod",
    "link-core": "npm link dist/core && npm link @ngx-gallery/core",
    "link-lightbox": "npm link dist/lightbox && npm link @ngx-gallery/lightbox",
    "link-gallerize": "npm link dist/gallerize && npm link @ngx-gallery/gallerize",
    "publish-all": "node .config/publish.js",
    "version": "node .config/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.6",
    "@angular/cdk": "^6.3.0",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/material": "^6.3.0",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@ngx-progressbar/core": "^5.0.1",
    "@ngx-progressbar/http": "^5.0.1",
    "@ngx-progressbar/router": "^5.0.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-highlightjs": "^2.1.1",
    "ngx-scrollbar": "^2.1.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cli": "^6.0.6",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/language-service": "^6.0.6",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^7.0.7",
    "@types/node-sass": "^3.10.32",
    "ansi-colors": "^2.0.1",
    "autoprefixer": "^8.5.1",
    "codelyzer": "~4.2.1",
    "cssnano": "^3.10.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.3",
    "postcss": "^6.0.22",
    "postcss-strip-inline-comments": "^0.1.5",
    "protractor": "~5.3.0",
    "replace-ext": "^1.0.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
