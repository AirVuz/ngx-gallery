{
  "name": "ngx-gallery",
  "version": "4.0.0-beta.1",
  "description": "Angular gallery directive that hooks the lightbox with the images automatically.",
  "homepage": "http://github.com/murhafsousli/ngx-gallery",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-gallery.git"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-gallery/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-demo": "ng serve demo",
    "start-demo-aot": "ng serve demo --aot",
    "build-demo": "ng build demo --prod --base-href https://murhafsousli.github.io/ngx-gallery/",
    "init-build": "npm run build-core && npm run version && npm run link-core && npm run build-lightbox && npm run version && npm run link-lightbox && npm run build-gallerize && npm run version && npm run link-gallerize && npm run version",
    "build-all": "ng build core --prod && ng build lightbox --prod && ng build gallerize --prod && npm run version",
    "build-core": "ng build core",
    "build-lightbox": "ng build lightbox",
    "build-gallerize": "ng build gallerize",
    "link-core": "npm link dist/core && npm link @ngx-gallery/core",
    "link-lightbox": "npm link dist/lightbox && npm link @ngx-gallery/lightbox",
    "link-gallerize": "npm link dist/gallerize && npm link @ngx-gallery/gallerize",
    "publish-all": "node .config/publish.js",
    "version": "node .config/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@ngx-progressbar/core": "^5.2.0",
    "@ngx-progressbar/http": "^5.2.0",
    "@ngx-progressbar/router": "^5.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-highlightjs": "^2.1.4",
    "ngx-scrollbar": "^3.2.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.6",
    "@angular-devkit/build-ng-packagr": "~0.10.6",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/language-service": "^7.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "ansi-colors": "^3.0.5",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.0",
    "replace-ext": "^1.0.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.33.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
